{
	"name": "@maduck/frontend",
	"private": true,
	"version": "0.1.0",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"start:dev": "concurrently -k \"next dev -p $(grep '^PORT=' <.env.development | cut -d= -f2)\"",
		"typecheck": "echo frontend && tsc --pretty",
		"typecheck:watch": "yarn typecheck --watch",
		"codegen": "ts-graphql-plugin typegen",
		"codegen:watch": "nodemon --ext ts,tsx --exec yarn codegen --ignore '/__generated__/'",
		"lint": "eslint --cache --ext .ts,.tsx .",
		"prepare": "cross-env-shell ../../scripts/yarn-prepare.sh"
	},
	"dependencies": {
		"@apollo/client": "^3.6.9",
		"@react-hook/window-size": "^3.0.7",
		"@tailwindcss/typography": "^0.5.4",
		"autoprefixer": "^10.4.8",
		"framer-motion": "^6.5.1",
		"graphql": "^16.5.0",
		"imagesloaded": "^5.0.0",
		"lightgallery": "^2.5.0",
		"lodash": "^4.17.21",
		"next": "12.2.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"styled-components": "^5.3.5",
		"tailwindcss": "^3.1.7",
		"twin.macro": "^2.8.2",
		"web-vitals": "^2.1.4",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@next/eslint-plugin-next": "^12.2.3",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@types/imagesloaded": "^4.1.2",
		"@types/jest": "^28.1.6",
		"@types/node": "^18.6.3",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/styled-components": "^5.1.25",
		"babel-jest": "^28.1.3",
		"babel-loader": "^8.2.5",
		"babel-plugin-styled-components": "^2.0.7",
		"babel-plugin-twin": "^1.0.2",
		"babel-preset-react-app": "^10.0.1",
		"concurrently": "^7.3.0",
		"cross-env": "^7.0.3",
		"eslint-plugin-graphql": "^4.0.0",
		"nodemon": "^2.0.19",
		"postcss": "^8.4.14",
		"ts-graphql-plugin": "^2.1.8",
		"typescript": "4.7.4"
	}
}
